<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
 http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
	version="1.0">
	<persistence-unit name="myUnit" transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>clases.Administrador</class>
		<class>clases.Denuncia</class>
		<class>clases.Evento</class>
		<class>clases.Mensaje</class>
		<class>clases.Negativo</class>
		<class>clases.Usuario</class>		
		<class>clases.Pasajero</class>
		<class>clases.Positivo</class>
		<class>clases.Recorrido</class>
		<class>clases.Telefono</class>
		<class>clases.Voto</class>		
	</persistence-unit>
</persistence>
